{
    "$schema": "https://swc.rs/schema.json",
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "decorators": true,
            "dynamicImport": true
        },
        "transform": {
            "decoratorVersion": "2022-03"
        },
        "target": "es2022",
        "externalHelpers": false,
        "loose": true
    },
    "module": {
        "type": "commonjs"
    },
    "sourceMaps": true,
    "inlineSourcesContent": true,
    "minify": false
}